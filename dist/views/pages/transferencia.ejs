<%- include("../partials/header"); %>



    <section class="container form_transf mt-5">
        <div class="row">
            <div class="col">
                <form class="form" action="/transferencia" method="POST">
                    <div class="form-group">
                        <label for="agencia">Agência</label>
                        <input class="form-control" type="text" id="agencia" name="agencia"
                            placeholder="Digite a agência" required>
                    </div>
                    <div class="form-group">
                        <label for="conta">Conta</label>
                        <input class="form-control" type="text" id="conta" name="conta" placeholder="Digite a conta"
                            required>
                    </div>
                    <div class="form-group">
                        <label for="valor">Valor</label>
                        <input class="form-control" type="number" min="0.01" step="0.01" id="valor" name="valor"
                            placeholder="R$ 0.01" required>
                    </div>

                    <input type="hidden" name="id" id="id" value="614f4333462d4b37ccdc2d17">
                    <input class="btn btn-success btn_transf" type="submit" value="CONFIRMAR">
                </form>
            </div>
        </div>

       
    </section>



    <%- include("../partials/footer"); %>